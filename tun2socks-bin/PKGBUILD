pkgname=tun2socks-bin
_pkgname=tun2socks
pkgver=2.4.1
pkgrel=1
pkgdesc="tun2socks - powered by gVisor TCP/IP stack"
arch=('x86_64')
url="https://github.com/xjasonlyu/tun2socks"
license=('GPL3')
groups=('modified')
conflicts=('tun2socks')
source=("${_pkgname}-linux-amd64-${pkgver}.zip::${url}/releases/download/v${pkgver}/${_pkgname}-linux-amd64.zip")
sha256sums=('4e025470453cc0b4c72b384b1441ecbeba1cf3b2ae98837ba630aa5102d1b6d0')

package() {
  install -Dm755 "${srcdir}/tun2socks-linux-amd64" "${pkgdir}"/usr/bin/"${_pkgname}"
}
