# Maintainer: h3fang

pkgname=yacd
pkgver=0.2.11
pkgrel=1
pkgdesc="Yet Another Clash Dashboard"
arch=('any')
url="https://github.com/haishanh/yacd"
source=("${pkgname}_${pkgver}.zip::https://github.com/haishanh/yacd/archive/gh-pages.zip")
sha256sums=('94478d2374ed4cfdafc5b9b211312ae47eb87478857da52ab9707dcc8dccd258')
groups=('modified')

package() {
  install -dm755 "${pkgdir}/usr/share"
  cp -rT "${srcdir}/yacd-gh-pages" "${pkgdir}/usr/share/${pkgname}"
}
